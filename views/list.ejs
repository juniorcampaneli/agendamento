<h1>Consultas</h1>
<hr>
<form action="/searchresult" method="get">
    <input type="search" name="search" id="search" placeholder="Digite o e-mail ou CPF">
    <button>Buscar</button>
</form>
<hr>
<% result.forEach(appo=>{ %>
    <h2>Consulta: <%=appo.description %></h2>
    <h4>Paciente: <%=appo.name %></h4>
    <h4>E-mail: <%=appo.email %></h4>
    <h4>CPF: <%=appo.cpf %></h4>
    <hr>
<% }) %>    